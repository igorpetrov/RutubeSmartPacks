<dune_plugin>
  <name>nowru</name>
  <caption>NOW.RU</caption>
  <entry_points>
    <entry_point>
      <parent_media_url>root://applications</parent_media_url>
      <media_url>nowru_app</media_url>
      <caption>NOW.RU</caption>
      <icon_url>plugin_file://icon.png</icon_url>
      <actions>
        <key_enter>
          <type>launch_media_url</type>
          <data>
            <url>www://https://rutube.ru/tv-release/nowru.server-13.2.0-01/index.html:::fullscreen=1&amp;webapp_keys=1&amp;zoom_level=100&amp;osd_size=1280x720&amp;overscan=0&amp;background_color=white&amp;input_handler=0</url>
          </data>
        </key_enter>
      </actions>
      <ip_address_required>yes</ip_address_required>
      <show_by_default>yes</show_by_default>
    </entry_point>
  </entry_points>
  <type>php</type>
</dune_plugin>